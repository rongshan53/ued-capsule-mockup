      },
      "edgeToEdgeEnabled": true,
      "package": "com.rongshan.CapsuleApps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "d393af1f-0ee5-4abb-8ee3-e1e4b9207e12"
      }
    },
    "owner": "rongshan"
